}
class Supervisor extends Empleado {
    double bono;

    public Supervisor(String nombre, String id, double salarioBase, double bono) {
        super(nombre, id, salarioBase);
        this.bono = bono;
    }

    @Override
    public double calcularSalario() {
       
        return salarioBase + bono;
    }

    public void promocionar(Empleado empleado) {
 
    }
}

