1)

import java.util.Scanner;
class Hora {
    private int horas;
    private int minutos;
    private int segundos;

    public Hora() {
        this.horas = 0;
        this.minutos = 0;
        this.segundos = 0;
    }

    public Hora(int horas, int minutos, int segundos) {
        this.horas = horas;
        this.minutos = minutos;
        this.segundos = segundos;
    }

    public void visualizarHora() {
        System.out.printf("%02d:%02d:%02d\n", this.horas, this.minutos, this.segundos);
    }

    public Hora sumarHoras(Hora h1, Hora h2) {
        int totalSegundos = this.segundos + h1.segundos + h2.segundos;
        int segundosRes = totalSegundos % 60;
        int totalMinutos = this.minutos + h1.minutos + h2.minutos + (totalSegundos / 60);
        int minutosRes = totalMinutos % 60;
        int horasRes = this.horas + h1.horas + h2.horas + (totalMinutos / 60);

        return new Hora(horasRes, minutosRes, segundosRes);
    }
}

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        Hora h1 = new Hora(1, 30, 45);
        Hora h2 = new Hora(2, 15, 30);
        Hora h3 = new Hora();

        h3 = h3.sumarHoras(h1, h2);

        System.out.println("Valor de h1: ");
        h1.visualizarHora();

        System.out.println("Valor de h2: ");
        h2.visualizarHora();

        System.out.println("Resultado de la suma: ");
        h3.visualizarHora();
    }
}


2) y 3)


class Empleado {
    private String nombre;
    private int numeroEmpleado;

    public Empleado(String nombre, int numeroEmpleado) {
        this.nombre = nombre;
        this.numeroEmpleado = numeroEmpleado;
    }

    public void verDatos() {
        System.out.println("Nombre del empleado: " + this.nombre);
        System.out.println("NÃºmero de empleado: " + this.numeroEmpleado);
    }
}

public class Main {
    public static void main(String[] args) {
        Empleado[] empleados = new Empleado[50];

        String[] nombres = {"Juan", "Maria", "Pedro", "Ana", "Luis", "Carla", "Jorge", "Laura", "Pablo", "Rosa",
                            "Gabriel", "Sofia", "Fernando", "Elena", "Diego", "Valeria", "Raul", "Marta", "Andres", "Clara",
                            "Roberto", "Carmen", "Ricardo", "Eva", "Alejandro", "Julia", "Ramon", "Cecilia", "Manuel", "Isabel",
                            "Daniel", "Monica", "Hector", "Patricia", "Oscar", "Silvia", "Victor", "Teresa", "Emilio", "Diana",
                            "Mario", "Olga", "Alberto", "Raquel", "Ernesto", "Natalia", "Felix", "Lorena", "Gonzalo", "Beatriz"};

        for (int i = 0; i < empleados.length; i++) {
            empleados[i] = new Empleado(nombres[i], 1 + i);
        }

        System.out.println("Datos de los empleados: ");
        for (int i = 0; i < empleados.length; i++) {
            System.out.println("Empleado " + (i + 1) + ": ");
            empleados[i].verDatos();
            System.out.println();
        }
    }
}



4) 

public class Hora {
    private int horas;
    private int minutos;
    private int segundos;

    public Hora(int horas, int minutos, int segundos) {
        this.horas = horas;
        this.minutos = minutos;
        this.segundos = segundos;
    }

    public int getHoras() {
        return horas;
    }

    public int getMinutos() {
        return minutos;
    }

    public int getSegundos() {
        return segundos;
    }

    public void avanzar() {
        segundos++;
        if (segundos >= 60) {
            segundos = 0;
            minutos++;
            if (minutos >= 60) {
                minutos = 0;
                horas++;
                if (horas >= 24) {
                    horas = 0;
                }
            }
        }
    }

    public void ponerACero() {
        this.horas = 0;
        this.minutos = 0;
        this.segundos = 0;
    }

    public void visualizar() {
        System.out.println("Hora actual: " + horas + ":" + minutos + ":" + segundos);
    }

    public static void main(String[] args) {
    }
}



5) 

public class Cadena {
    private int longitud;
    private String cadena;

    public Cadena(String cadena) {
        this.cadena = cadena;
        this.longitud = cadena.length();
    }

    public int getLongitud() {
        return longitud;
    }

    public String getCadena() {
        return cadena;
    }

    public char caracter(int i) {
        if (i >= 0 && i < longitud) {
            return cadena.charAt(i);
        } else {
            return '\0'; 
        }
    }

    public void visualizar() {
        System.out.println("Cadena: " + cadena);
        System.out.println("Longitud: " + longitud);
    }

    public static void main(String[] args) {
        
    }
}


